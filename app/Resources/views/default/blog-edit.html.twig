{% extends 'base.html.twig' %}

{% block content %}

{{ form_start(form) }}
    {{ form_errors(form) }}
    <div class="form-group">
        {{ form_row(form.slug) }}
    </div>
    <div class="form-group">
        {{ form_row(form.title) }}
    </div>
    <div class="form-group">
        {{ form_row(form.published) }}
    </div>
    <div class="form-group">
        {{ form_row(form.content) }}
    </div>
    <div class="form-group">
        {{ form_row(form.tags) }}
    </div>
    <div class="form-group">
        {{ form_widget(form.save) }}
    </div>
{{ form_end(form) }}

{% endblock %}
