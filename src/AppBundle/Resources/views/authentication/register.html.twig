{% extends 'AppBundle::base.html.twig' %}

{% block content %}

{{ form_start(form) }}
    {{ form_errors(form) }}
    <div class="form-group">
        {{ form_row(form.username) }}
    </div>
    <div class="form-group">
        {{ form_row(form.password) }}
    </div>
    <div class="form-group">
        {{ form_row(form.email) }}
    </div>
    <div class="form-group">
        {{ form_widget(form.save) }}
    </div>
{{ form_end(form) }}

{% endblock %}
